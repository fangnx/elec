{
  "name": "smoretify",
  "version": "1.0.0",
  "private": true,
  "author": "Naxin Fang",
  "license": "MIT",
  "engine": {
    "node": "10.x"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "axios": "^0.19.0",
    "babel": "^6.23.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "electron-packager": "^14.0.5",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "foreman": "^3.0.1",
    "genius-api": "^0.4.1",
    "passport": "^0.4.0",
    "passport-spotify": "^1.0.1",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "spotify-web-api-node": "^4.0.0",
    "url-parse": "^1.5.8"
  },
  "devDependencies": {},
  "scripts": {
    "postinstall": "npm run build-client",
    "build-client": "cd ./client/ && npm install && npm run build && cd ..",
    "client": "cd ./client/ && npm start",
    "electron": "cd ./client/ && npm run electron",
    "server": "babel-node server/server.js",
    "start": "npm run server",
    "start-electron": "concurrently --kill-others-on-fail \"npm run server\" \"npm run electron\"",
    "dev": "NODE_ENV=dev concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev-electron": "NODE_ENV=dev concurrently --kill-others-on-fail \"npm run server\" \"npm run electron\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "electronMain": "./client/src/electron/main.js"
}
